{
  "name": "@fritx/node-ts-demo",
  "version": "0.0.0",
  "main": "./dist/server",
  "scripts": {
    "start": "run-s build node:prod",
    "dev": "run-p watch nodemon",
    "nodemon": "sleep 5 && nodemon -d 3 -w dist --exec 'npm run node:dev'",
    "node:dev": "NODE_ENV=development dotenv node .",
    "node:prod": "NODE_ENV=production dotenv node .",
    "test": "dotenv -e .env -e .env.test jest",
    "test:cov": "npm run test -- -- --coverage",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "watch": "npm run clean && tsc --watch",
    "build": "npm run clean && tsc"
  },
  "dependencies": {
    "dotenv-cli": "^4.0.0",
    "ioredis": "^4.17.3",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-helmet": "^6.0.0",
    "koa-ratelimit": "^4.2.1",
    "koa-redis": "^4.0.1",
    "koa-router": "^9.4.0",
    "koa-session": "^6.1.0",
    "koa-static": "^5.0.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@types/ioredis": "^4.17.5",
    "@types/jest": "^26.0.14",
    "@types/koa": "^2.11.5",
    "@types/koa-helmet": "^6.0.0",
    "@types/koa-ratelimit": "^4.2.2",
    "@types/koa-redis": "^4.0.0",
    "@types/koa-router": "^7.4.1",
    "@types/koa-session": "^5.10.3",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^12.0.10",
    "@types/source-map-support": "^0.5.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.3",
    "node": "^12.19.0",
    "nodemon": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
